{
  "name": "curriculum-vitae",
  "version": "1.10.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@sineverba/years-interval": "^2.1.0",
    "bootstrap": "^5.3.2",
    "highcharts": "^11.2.0",
    "highcharts-react-official": "^3.2.1",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.2",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.1"
  },
  "scripts": {
    "msw": "REACT_APP_MSW=true react-scripts start",
    "start": "react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN='true' REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "CI=true ENV=test react-scripts test --coverage",
    "eslint": "npx eslint .",
    "fixeslint": "npx eslint . --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "msw": "^1.2.2",
    "npm-check-updates": "^16.14.12"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js",
      "!**/__mocks__/**",
      "!**/__utils__/**"
    ],
    "collectCoverageFrom": [
      "!<rootDir>/src/reportWebVitals.js",
      "!<rootDir>/src/assets/*",
      "!<rootDir>/src/__tests__/__snapshots__/*.snap"
    ]
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
